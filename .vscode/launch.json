{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "program": "${workspaceRoot}/dist/server/index.js",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "runtimeVersion": "12.14.0",
      "runtimeArgs": [], // ["--prof"],
      "env": {
        "NODE_ENV": "development"
      },
      "args": [],
      "stopOnEntry": false,
      "restart": false,
      "sourceMaps": true,
      // "sourceMapPathOverrides": {
      //   "webpack:///./~/*": "${workspaceRoot}/node_modules/*",
      //   "webpack:///./*": "${workspaceRoot}/*",
      //   "webpack:///*": "*"
      // },
      // "outFiles": [],
      // "envFile": "${workspaceFolder}/.env",
      // "trace": "verbose",
      "preLaunchTask": "build",
      // "postDebugTask": "",
      "autoAttachChildProcesses": true,
      "protocol": "inspector",
      "showAsyncStacks": true,
      // "address": "localhost",
      "smartStep": true,
      "skipFiles": ["<node_internals>/**"],
      // "disableOptimisticBPs": true,
      // "outputCapture": "console",
      // "internalConsoleOptions": "openOnSessionStart",
      "console": "internalConsole"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "cwd": "${workspaceFolder}",
      "processId": "${command:PickProcess}",
      "port": 9229,
      "stopOnEntry": false,
      "restart": false,
      "sourceMaps": true,
      // "trace": "verbose",
      "preLaunchTask": "build",
      "protocol": "inspector",
      "showAsyncStacks": true,
      "smartStep": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via npm",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeVersion": "12.14.0",
      "runtimeArgs": ["start", "--silent", "--", "--nolazy", "--inspect=9229"],
      "port": 9229,
      "stopOnEntry": false,
      "restart": false,
      "sourceMaps": true,
      // "trace": "verbose",
      "preLaunchTask": "build",
      "autoAttachChildProcesses": true,
      "protocol": "inspector",
      "showAsyncStacks": true,
      "smartStep": true,
      "skipFiles": ["<node_internals>/**"],
      "console": "internalConsole"
    }
  ]
}
